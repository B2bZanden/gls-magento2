<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <tig_gls>
                <name>GLS</name>
                <model>TIG\GLS\Model\Carrier\GLS</model>
                <title>Ship with GLS</title>
                <active>1</active>
                <allowed_methods>gls_default,gls_express_t9,gls_express_t12,gls_express_t17,gls_saturday,gls_saturday_express_t9,gls_saturday_express_t12,gls_saturday_express_t17</allowed_methods>
                <ship_allow_specific>0</ship_allow_specific>
            </tig_gls>
        </carriers>
        <tig_gls>
            <stability />
            <supported_magento_version>2.2.0 - 2.2.9, 2.3.0 - 2.3.2</supported_magento_version>
            <api>
                <live_base_url>https://api.gls.nl/V1/api/</live_base_url>
                <test_base_url>https://api.gls.nl/Test/V1/api/</test_base_url>
            </api>
        </tig_gls>
    </default>
</config>
